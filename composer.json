{
    "name": "morbihanet/modeler",
    "description": "Dynamic models for Laravel 5.8 and up",
    "keywords": [
        "morbihanet",
        "laravel",
        "databse",
        "model",
        "dynamic"
    ],
    "homepage": "https://github.com/morbihanet/laravel-modeler",
    "license": "MIT",
    "authors": [
        {
            "name": "GÃ©rald Plusquellec",
            "email": "contact@morbihanet.com",
            "homepage": "http://www.morbihanet.com",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
          "type": "vcs",
          "url": "git@github.com:schpill/laravel-mongodb.git"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "josiasmontag/laravel-redis-mock": "^1.2",
        "laravel/framework": "^5.8|^6.0|^7.0"
    },
    "require-dev": {
        "ext-mongodb": "*",
        "filp/whoops": "*",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "pagerfanta/pagerfanta": "^2.1",
        "phpstan/phpstan": "^0.12.36",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1",
        "schpill/mongodb": "*",
        "sentry/sentry-laravel": "*"
    },
    "autoload": {
        "psr-4": {
            "Morbihanet\\Modeler\\": "src"
        },
        "files": [
          "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Morbihanet\\Modeler\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Morbihanet\\Modeler\\ModelerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
